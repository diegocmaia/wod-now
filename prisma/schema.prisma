generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Workout {
  id             String  @id @default(cuid())
  title          String
  timeCapSeconds Int
  equipment      String
  data           String
  isPublished    Boolean @default(false)

  @@index([isPublished], map: "idx_workouts_is_published")
  @@index([id], map: "idx_workouts_id_lookup")
}
