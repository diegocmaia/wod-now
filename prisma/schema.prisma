generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Workout {
  id             String  @id @default(cuid())
  title          String
  timeCapSeconds Int
  equipment      String
  data           String
  isPublished    Boolean @default(false)

  @@index([isPublished], map: "idx_workouts_is_published")
  @@index([id], map: "idx_workouts_id_lookup")
  @@index([isPublished, id], map: "idx_workouts_is_published_id")
  @@index([isPublished, timeCapSeconds, id], map: "idx_workouts_is_published_timecap_id")
}
